SKIP:
{
    " "
  | "\t"
  | "\n"
  | "\r"
  | <"%" (~["\n","\r"])* ("\n"|"\r")>
}

TOKEN:
{
    < LAMBDA : "lambda" >
  | < LET    : "let"    >
}

TOKEN:
{
    < LPAR : "(" >
  | < RPAR : ")" >
  | < NUMBER : (["-"])? (["0"-"9"])+ ("." (["0"-"9"])+)?
              |(["-"])? "." (["0"-"9"])+ >
  | < OP:    ["-", "+", "*", "/"] >
  | < SYMBOL : ["a"-"z", "A"-"Z", "~", "`", "!", "@", "#", "$", "/", "^", "&",
                "*", "_", "-", "=", "+", "{", "}", "[", "]", "|", "\\", ":",
                ";", "<", ">", ",", ".", "?", "'", "\""](["a"-"z", "A"-"Z",
                "0"-"9", "~", "`", "!", "@", "#", "$", "/", "^", "&", "*", "_",
                "-", "=", "+", "{", "}", "[", "]", "|", "\\", ":", ";", "<",
                ">", ",", ".", "?", "'", "\""])* >


}

TOKEN:
{
  < ERROR : ~[] >
}

