String expr() :
{
  Token n; 
  String v;
}
{
/* CASE : LET */
    LOOKAHEAD(2) <LPAR> 
                 <LET>
                 <LPAR><LPAR>   { String A = "((lambda (" + expr() + ") ";
                                  String B = expr(); }
                 <RPAR><RPAR>   { String C = expr(); }
                 <RPAR>         { A += C + ") " + B + ")"; return A; }

/* CASE : LAMBDA */
  | LOOKAHEAD(2) <LPAR>
                 <LAMBDA>
                 <LPAR>     { String A = "(lambda (" + expr() + ") "; }
                 <RPAR>     { A += expr() + ")"; }
                 <RPAR>     { return A; }

/* CASE : ELSEWISE */

  | LOOKAHEAD(2) <LPAR>
             n = <OP>       { String B = "(" + n.toString() + " " + expr(); }
           ( v = expr()     { B += " " + v; }  )+
                 <RPAR>     { B += ")"; return B; }

  | LOOKAHEAD(2) <LPAR>
             n = <SYMBOL>   { String app = "(" + n.toString(); }
           ( v = expr()     { app += " " + v; } )*
                 <RPAR>     { app += ")"; return app; }
  |          n = <NUMBER>   { String B = n.toString(); return B; } 
  |          n = <SYMBOL>   { String C = n.toString(); return C; }

}

int getVal() :
{ Token n; }
{
  <LPAR>
  <OP>
  n = <NUMBER> { int a = Integer.parseInt(n.toString()) ; }
  n = <NUMBER> { int b = Integer.parseInt(n.toString()) ; }
  {System.out.println( a + b) ; }
  <RPAR>
}  
String opVal() :
{Token n; }
{
  n = <SYMBOL> { return n.toString(); }
| n = <NUMBER> { return n.toString(); }
}



